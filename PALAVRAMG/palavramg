#include <iostream>
#include <string>
#include <ctype.h>

/*	http://br.spoj.com/problems/PALAVRMG/	*/
using namespace std;
 
int main(){
	int n, flag;
	string palavra;
	char atual, next;
	cin >> n;
	for( ; n > 0; n--){
		flag = 1;
		cin >> palavra;
		//cout << palavra << "( " << palavra.size() << " )" <<": ";
		cout << palavra << ": ";
		
		if(palavra.size() == 1){
			cout << "O" << endl;
			flag = 0;
		} else {
			atual = tolower(palavra[0]);	
			for ( int j = 1; j < palavra.size(); j++){
				next = tolower(palavra[j]);
				if (atual == next){
					cout << "N" << endl;
					flag = 0;
					break;
				} else if (atual > next){
					cout << "N" << endl;
					flag = 0;
					break;
				}
				atual = next;
			}	
		}
		if(flag == 1)
			cout << "O" << endl;
	}
	return 0;
}
